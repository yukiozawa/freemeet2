<%= form_for(@free_post)  do |f| %>
  <%= f.label :start_time, "フリータイム（開始）" %></br>
  <%= f.datetime_select :start_time %></br>
  <%= f.label :start_time, "フリータイム（終了）" %></br>
  <%= f.datetime_select :end_time %></br>
